{
  "id": "mintstack-finance",
  "realm": "mintstack-finance",
  "displayName": "MintStack Finance Portal",
  "enabled": true,
  "sslRequired": "external",
  "registrationAllowed": true,
  "loginWithEmailAllowed": true,
  "duplicateEmailsAllowed": false,
  "resetPasswordAllowed": true,
  "editUsernameAllowed": false,
  "bruteForceProtected": true,
  "permanentLockout": false,
  "maxFailureWaitSeconds": 900,
  "minimumQuickLoginWaitSeconds": 60,
  "waitIncrementSeconds": 60,
  "quickLoginCheckMilliSeconds": 1000,
  "maxDeltaTimeSeconds": 43200,
  "failureFactor": 5,
  "defaultSignatureAlgorithm": "RS256",
  "accessTokenLifespan": 28800,
  "accessTokenLifespanForImplicitFlow": 900,
  "ssoSessionIdleTimeout": 1800,
  "ssoSessionMaxLifespan": 36000,
  "ssoSessionIdleTimeoutRememberMe": 2592000,
  "ssoSessionMaxLifespanRememberMe": 2592000,
  "offlineSessionIdleTimeout": 2592000,
  "offlineSessionMaxLifespanEnabled": false,
  "offlineSessionMaxLifespan": 5184000,
  "accessCodeLifespan": 60,
  "accessCodeLifespanUserAction": 300,
  "accessCodeLifespanLogin": 1800,
  "actionTokenGeneratedByAdminLifespan": 43200,
  "actionTokenGeneratedByUserLifespan": 300,
  "roles": {
    "realm": [
      {
        "name": "user",
        "description": "Regular user role",
        "composite": false,
        "clientRole": false
      },
      {
        "name": "admin",
        "description": "Administrator role",
        "composite": true,
        "clientRole": false,
        "composites": {
          "realm": ["user"]
        }
      }
    ],
    "client": {
      "finance-backend": [
        {
          "name": "api-access",
          "description": "Access to API endpoints",
          "composite": false,
          "clientRole": true
        }
      ]
    }
  },
  "groups": [
    {
      "name": "Users",
      "path": "/Users",
      "realmRoles": ["user"]
    },
    {
      "name": "Administrators",
      "path": "/Administrators",
      "realmRoles": ["admin"]
    }
  ],
  "defaultGroups": ["/Users"],
  "requiredCredentials": ["password"],
  "otpPolicyType": "totp",
  "otpPolicyAlgorithm": "HmacSHA1",
  "otpPolicyInitialCounter": 0,
  "otpPolicyDigits": 6,
  "otpPolicyLookAheadWindow": 1,
  "otpPolicyPeriod": 30,
  "otpSupportedApplications": [
    "Google Authenticator",
    "Microsoft Authenticator",
    "FreeOTP"
  ],
  "passwordPolicy": "length(8) and upperCase(1) and lowerCase(1) and digits(1)",
  "clients": [
    {
      "clientId": "finance-backend",
      "name": "Finance Backend API",
      "description": "Spring Boot Backend Service",
      "enabled": true,
      "clientAuthenticatorType": "client-secret",
      "secret": "finance-backend-secret-2026",
      "redirectUris": [],
      "webOrigins": [],
      "bearerOnly": true,
      "consentRequired": false,
      "standardFlowEnabled": false,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": false,
      "serviceAccountsEnabled": false,
      "publicClient": false,
      "frontchannelLogout": false,
      "protocol": "openid-connect",
      "attributes": {
        "access.token.lifespan": "28800"
      },
      "fullScopeAllowed": true
    },
    {
      "clientId": "finance-frontend",
      "name": "Finance Frontend App",
      "description": "React Frontend Application",
      "enabled": true,
      "clientAuthenticatorType": "client-secret",
      "redirectUris": [
        "http://localhost:3000/*",
        "http://127.0.0.1:3000/*"
      ],
      "webOrigins": [
        "http://localhost:3000",
        "http://127.0.0.1:3000"
      ],
      "bearerOnly": false,
      "consentRequired": false,
      "standardFlowEnabled": true,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": true,
      "serviceAccountsEnabled": false,
      "publicClient": true,
      "frontchannelLogout": true,
      "protocol": "openid-connect",
      "attributes": {
        "pkce.code.challenge.method": "S256",
        "access.token.lifespan": "28800",
        "post.logout.redirect.uris": "http://localhost:3000/*"
      },
      "fullScopeAllowed": true,
      "defaultClientScopes": [
        "web-origins",
        "acr",
        "profile",
        "roles",
        "email"
      ],
      "optionalClientScopes": [
        "address",
        "phone",
        "offline_access"
      ]
    }
  ],
  "users": [
    {
      "username": "admin",
      "enabled": true,
      "emailVerified": true,
      "firstName": "Admin",
      "lastName": "User",
      "email": "admin@mintstack.local",
      "credentials": [
        {
          "type": "password",
          "value": "Admin123!",
          "temporary": false
        }
      ],
      "realmRoles": ["admin"],
      "groups": ["/Administrators"]
    },
    {
      "username": "testuser",
      "enabled": true,
      "emailVerified": true,
      "firstName": "Test",
      "lastName": "User",
      "email": "testuser@mintstack.local",
      "credentials": [
        {
          "type": "password",
          "value": "Test123!",
          "temporary": false
        }
      ],
      "realmRoles": ["user"],
      "groups": ["/Users"]
    }
  ],
  "components": {
    "org.keycloak.storage.UserStorageProvider": [
      {
        "name": "ldap",
        "providerId": "ldap",
        "subComponents": {
          "org.keycloak.storage.ldap.mappers.LDAPStorageMapper": [
            {
              "name": "username",
              "providerId": "user-attribute-ldap-mapper",
              "config": {
                "ldap.attribute": ["uid"],
                "is.mandatory.in.ldap": ["true"],
                "read.only": ["false"],
                "always.read.value.from.ldap": ["false"],
                "user.model.attribute": ["username"]
              }
            },
            {
              "name": "first name",
              "providerId": "user-attribute-ldap-mapper",
              "config": {
                "ldap.attribute": ["cn"],
                "is.mandatory.in.ldap": ["true"],
                "read.only": ["false"],
                "always.read.value.from.ldap": ["false"],
                "user.model.attribute": ["firstName"]
              }
            },
            {
              "name": "last name",
              "providerId": "user-attribute-ldap-mapper",
              "config": {
                "ldap.attribute": ["sn"],
                "is.mandatory.in.ldap": ["true"],
                "read.only": ["false"],
                "always.read.value.from.ldap": ["false"],
                "user.model.attribute": ["lastName"]
              }
            },
            {
              "name": "email",
              "providerId": "user-attribute-ldap-mapper",
              "config": {
                "ldap.attribute": ["mail"],
                "is.mandatory.in.ldap": ["false"],
                "read.only": ["false"],
                "always.read.value.from.ldap": ["false"],
                "user.model.attribute": ["email"]
              }
            }
          ]
        },
        "config": {
          "enabled": ["true"],
          "vendor": ["other"],
          "connectionUrl": ["ldap://openldap:389"],
          "bindDn": ["cn=admin,dc=mintstack,dc=local"],
          "bindCredential": ["LdapAdmin2026!"],
          "usersDn": ["ou=users,dc=mintstack,dc=local"],
          "usernameLDAPAttribute": ["uid"],
          "rdnLDAPAttribute": ["uid"],
          "uuidLDAPAttribute": ["entryUUID"],
          "userObjectClasses": ["inetOrgPerson"],
          "editMode": ["WRITABLE"],
          "syncRegistrations": ["true"],
          "searchScope": ["1"],
          "pagination": ["true"],
          "importEnabled": ["true"],
          "batchSizeForSync": ["1000"],
          "fullSyncPeriod": ["-1"],
          "changedSyncPeriod": ["-1"],
          "cachePolicy": ["DEFAULT"],
          "priority": ["0"],
          "validatePasswordPolicy": ["false"],
          "trustEmail": ["false"],
          "useTruststoreSpi": ["ldapsOnly"],
          "connectionPooling": ["true"],
          "startTls": ["false"],
          "useKerberosForPasswordAuthentication": ["false"]
        }
      }
    ]
  },
  "smtpServer": {},
  "internationalizationEnabled": true,
  "supportedLocales": ["en", "tr"],
  "defaultLocale": "en",
  "browserSecurityHeaders": {
    "contentSecurityPolicyReportOnly": "",
    "xContentTypeOptions": "nosniff",
    "referrerPolicy": "no-referrer",
    "xRobotsTag": "none",
    "xFrameOptions": "SAMEORIGIN",
    "contentSecurityPolicy": "frame-src 'self'; frame-ancestors 'self'; object-src 'none';",
    "xXSSProtection": "1; mode=block",
    "strictTransportSecurity": "max-age=31536000; includeSubDomains"
  }
}
